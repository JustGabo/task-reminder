{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "npm run build:web",
  "outputDirectory": "dist",
  "framework": "create-react-app"
} 